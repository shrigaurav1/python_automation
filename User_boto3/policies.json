{
  "managed_policies": [
    "arn:aws:iam::aws:policy/AmazonS3ReadOnlyAccess",
    "arn:aws:iam::aws:policy/IAMUserChangePassword"
  ],
  "custom_policies": [
    {
      "name": "EC2DescribeOnlyPolicy",
      "document": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "ec2:Describe*"
            ],
            "Resource": "*"
          }
        ]
      }
    },
    {
      "name": "S3ListBucketsPolicy",
      "document": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": "s3:ListAllMyBuckets",
            "Resource": "*"
          }
        ]
      }
    }
  ]
}
